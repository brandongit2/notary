  "scripts": {
    "start:dev": "docker compose up -d && nest start --watch --preserveWatchOutput",
    "start:prod": "node dist/main",
    "build": "nest build",
    "dbsetup": "cd knex && knex migrate:latest && knex seed:run",
    "psql": "docker exec -it notary-db psql -U postgres -d notary",
    "knex": "cd knex && knex --",
    "redis": "docker exec -it notary-sessions redis-cli",
    "lint": "prettier --write . --loglevel warn && eslint --fix . && tsc"
  },
  "dependencies": {
    "@brandonnpm2/nestjs-knex": "2.1.0",
    "@brandonnpm2/nestjs-redis": "1.1.0",
