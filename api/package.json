    "@nestjs/core": "8.0.6",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/platform-fastify": "8.0.6",
    "@types/bcrypt": "5.0.0",
    "@types/faker": "5.5.8",
    "@types/ioredis": "4.26.7",
    "@types/memoizee": "0.4.6",
    "@types/node": "16.7.1",
    "@types/uuid": "8.3.1",
    "apollo-server-core": "3.1.2",
    "apollo-server-fastify": "3.1.2",
    "bcrypt": "5.0.1",
    "class-transformer": "0.4.0",
