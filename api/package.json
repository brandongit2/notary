    "graphql": "15.5.1",
    "graphql-tools": "8.1.0",
    "ioredis": "4.27.8",
    "knex": "0.95.10",
    "pg": "8.7.1",
    "prettier": "2.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.3.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.3.5",
    "uuid": "8.3.2"
  }
